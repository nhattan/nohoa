<%= search_form_for [:admin, @search] do |f| %>
  <table class="table">
    <tr class="field">
      <th><%= f.label :email_cont, "Email" %></th>
      <td><%= f.text_field :email_cont %></td>
    </tr>

    <tr class="field">
      <th><%= f.label :roles, "Role" %></th>
      <td><%= f.select :user_roles_role_id_eq, Role.all.map{|r| [r.name, r.id]}, include_blank: true %></td>
    </tr>

    <tr class="actions"><th colspan="2"><%= f.submit "Search", class: "btn btn-warning" %></th></tr>
  </table>
<% end %>